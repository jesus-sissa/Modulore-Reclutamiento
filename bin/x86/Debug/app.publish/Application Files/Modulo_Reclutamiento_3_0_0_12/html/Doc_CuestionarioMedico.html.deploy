	<script type="text/javascript">
		window.chrome.webview.addEventListener('message', event => {
			WriteDataFromCsharp(event.data);
		});

		function WriteDataFromCsharp(data) {

			cuestionario = JSON.parse(data);
			document.getElementById('logo').setAttribute("src", cuestionario.FotoLogo);
			document.getElementById('firma').setAttribute("src", cuestionario.Firma);
			cuestionarioMed = cuestionario.CuestionarioMedico

			document.getElementById('dia').innerText = formatoDocumento(cuestionarioMed.FechaRegistro, "DD");
			document.getElementById('mes').innerText = formatoDocumento(cuestionarioMed.FechaRegistro, "MM");
			document.getElementById('año').innerText = formatoDocumento(cuestionarioMed.FechaRegistro, "AA");

			infopersonal = cuestionario.CuestionarioMedico.Inforpersonal;
			document.getElementById('nombre').innerText = infopersonal.Nombre;
			document.getElementById('puesto').innerText = infopersonal.Puesto;
			document.getElementById('edad').innerText = infopersonal.Edad;
			asignarCheck(infopersonal.Genero)
			document.getElementById('ncdia').innerText = formatoDocumento(infopersonal.FechaNacimiento, "DD");
			document.getElementById('ncmes').innerText = formatoDocumento(infopersonal.FechaNacimiento, "MM");
			document.getElementById('ncaño').innerText = formatoDocumento(infopersonal.FechaNacimiento, "AA");
			asignarCheck(infopersonal.EstadoCivil.replace(' ', ''))
			document.getElementById('lugarNacimiento').innerText = infopersonal.LugarNacimiento;
			document.getElementById('estadoNacimiento').innerText = infopersonal.EstadoNacimiento;
			document.getElementById('calle').innerText = infopersonal.Calle;
			document.getElementById('numero').innerText = infopersonal.Numero;
			document.getElementById('colonia').innerText = infopersonal.Colonia;
			document.getElementById('municipio').innerText = infopersonal.Ciudad;
			document.getElementById('cp').innerText = infopersonal.Cp;
			document.getElementById('estado').innerText = infopersonal.Estado;
			document.getElementById('telefono').innerText = infopersonal.Telefono;
			document.getElementById('celular').innerText = infopersonal.Celular;
			document.getElementById('numerohijos').innerText = infopersonal.NoHijos;
			document.getElementById('edadeshijos').innerText = infopersonal.EdadHijos;
			//estan sanos falta
			asignarCheck(infopersonal.Complexion);

			document.getElementById('peso').innerText = infopersonal.Peso;
			document.getElementById('estatura').innerText = infopersonal.Altura;

			antecedente = cuestionario.CuestionarioMedico.AntecedentesMedicos;
			asignarCheck("lop" + antecedente.UsaLentesoPupilentes);
			if (antecedente.UsaLentesoPupilentes === "S") {
				document.getElementById('dialop').innerText = formatoDocumento(antecedente.Desdecuando, "DD");
				document.getElementById('meslop').innerText = formatoDocumento(antecedente.Desdecuando, "MM");
				document.getElementById('añolop').innerText = formatoDocumento(antecedente.Desdecuando, "AA");

				if (antecedente.RespuestaLentesPupilentes === "0") {
					asignarCheck("lop_1")
				} else {
					asignarCheck("lop_" + antecedente.RespuestaLentesPupilentes)
				}

			}

			for (var i = 0; i < antecedente.OpcionesAntecedentesFamiliares.length; i++) {
				agregarCheck("antecedentesFamiliares", "af", antecedente.OpcionesAntecedentesFamiliares[i]);
			}


			agregarAntecedente("antecedentesmedicos", "ENFERMEDAD CONGENITA O HEREDITARIA (Nacimiento) :", "col-12", "ec", antecedente.EnfermedadCongenita, "N", null, "N", null)
			agregarhr("antecedentesmedicos")
			agregarAntecedente("antecedentesmedicos", "DEFORMIDAD CONGENITA (Nacimiento) :", "col-12", "dc", antecedente.DeformidadCongenita, "N", null, "N", null)
			agregarhr("antecedentesmedicos")
			agregarAntecedente("antecedentesmedicos", "ENFERMEDADES INFANTILES IMPORTANTES :", "col-12", "ei", antecedente.EnfermedadesInfantiles, "S", "rif", antecedente.OpcionesEnfermedadesInfantiles, "S", antecedente.OtraEnfermedadInfantil)
			agregarhr("antecedentesmedicos")
			agregarAntecedente2("antecedentesmedicos", "ALERGIAS :", "col-3", "al", antecedente.Alergias, "S", "col-9", "ral", antecedente.OpcionesAlergias)
			agregarhr("antecedentesmedicos")
			agregarAntecedenteT("antecedentesmedicos", "ENFERMEDADES DE LOS OJOS :", "col-12")
			agregarAntecedente("antecedentesmedicos", "USA USTED LENTES DE CONTACTO :", "col-12", "lc", antecedente.UsaLentesContacto, "S", "rlc", antecedente.OpcionesUsaLentesContacto, "N", null)
			const antectext = []; const antecresp = [];
			antectext[0] = "VE USTED BIEN :"; antecresp[0] = antecedente.VeUstedBien;
			antectext[1] = "OJO IZQUIERDO :"; antecresp[1] = antecedente.OjoIzquierdo;
			antectext[2] = "OJO DERECHO :"; antecresp[2] = antecedente.OjoDerecho;
			agregarAntecedente_3col("antecedentesmedicos", "col-4", antectext, antecresp, null)
			agregarAntecedente("antecedentesmedicos", "CIRUGIA OCULAR :", "col-12", "ode", antecedente.CirugiaOcular, "N", "", null, "N", null)
			const antetext2 = []; const anteresp2 = [];
			antetext2[0] = "POR : "; anteresp2[0] = antecedente.PorqueRealizoCirugiaOcular;
			antetext2[1] = "FECHA : "; anteresp2[1] = antecedente.FechaCirugia;
			agregarAntecedente_2coltext("antecedentesmedicos", antetext2, anteresp2, "col-6")
			agregarhr("antecedentesmedicos")
			agregarAntecedente("antecedentesmedicos", "ENFERMEDAD DE LOS OIDOS :", "col-12", "efo", antecedente.EnfermedadOidos, "N", "", null, "N", null)
			agregarAntecedenteT("antecedentesmedicos", "CUAL(es) : " + antecedente.CualEnfermedadOidos, "col-12")
			const antecescucha = []; const respescucha = [];
			antecescucha[0] = "OYE USTED BIEN :"; respescucha[0] = antecedente.EscuchaUstedBien;
			antecescucha[1] = "USA APARATO AUDITIVO :"; respescucha[1] = antecedente.UsaAparatoAuditivo;
			const extra = [];
			extra[0] = "DESDE : ";
			extra[1] = antecedente.DesdeCuandoUsaAparatoAuditivo;
			agregarAntecedente_3col("antecedentesmedicos", "col-4", antecescucha, respescucha, extra)
			agregarhr("antecedentesmedicos")
			agregarAntecedente("antecedentesmedicos", "ENFERMEDADES DENTALES :", "col-12", "efd", antecedente.EnfermedadesDentales, "S", "refd", antecedente.OpcionesEnfermedadesDentales, "N", null)
			agregarhr("antecedentesmedicos")
			agregarAntecedenteL("antecedentesmedicos", "ENFERMEDADES HORMONALES :", "col-4", "S", "col-8", "refh", antecedente.OpcionesEnfermedadesHormonales, "S", antecedente.OtrasEnfermedadesHormonales)
			agregarhr("antecedentesmedicos")
			agregarAntecedente("antecedentesmedicos", "ENFERMEDADES PULMONARES :", "col-12", "efd", antecedente.EnfermedadesPulmonares, "S", "refd", antecedente.OpcionesEnfermedadesPulmonares, "S", antecedente.OtrasEnfermedadesPulmonares)
			agregarhr("antecedentesmedicos")
			agregarAntecedente2("antecedentesmedicos", "ALTERACION DE LA PRESION ARTERIAL :", "col-6", "apa", antecedente.AlteracionPresionArterial, "S", "col-6", "rapa", antecedente.OpcionesAlteracionPresionArterial)
			agregarAntecedenteT("antecedentesmedicos", "MEDICAMENTOS : " + antecedente.MedicamentosAlteracionPresionArterial, "col-12")
			agregarAntecedente("antecedentesmedicos", "ESTOY EN CONTROL MEDICO :", "col-12", "cmapa", antecedente.ControlMedicoAlteracionPresionArterial, "S", "rcmapa", antecedente.OpcionescontrolMedicoAlteracionPresionArterial, "N", null)
			agregarhr("antecedentesmedicos")
			agregarAntecedente("antecedentesmedicos", "ENFERMEDADES DEL CORAZON :", "col-12", "efc", antecedente.EnfermadesCorazon, "S", "refc", antecedente.OpcionesEnfermedadesCorazon, "N", null)

			agregarAntecedente("antecedentesmedicos", "ENFERMEDADES DIGESTIVAS :", "col-12", "cmapa", antecedente.EnfermedadDigestiva, "S", "rcmapa", antecedente.OpcionEnfermedadesDigestivas, "N", null)
			agregarAntecedente("antecedentesmedicos2", "ENFERMEDADES DEL HIGADO Y/O VIAS BILIARES :", "col-12", "efh", antecedente.EnfermedadHigado, "S", "refh", antecedente.OpcionesEnfermedadesHigado, "N", null)
			agregarhr("antecedentesmedicos2")
			agregarAntecedente("antecedentesmedicos2", "DIABETTES MELLITUS/AZUCAR :", "col-12", "efh", antecedente.DiabetesMellitus, "S", "refh", antecedente.OpcionesDiabetesMelitus, "N", null)
			agregarAntecedenteT("antecedentesmedicos2", "FECHA ULTIMO EXAMEN DE SANGRE : " + antecedente.FechaUltimoExamenSangre, "col-12")
			agregarAntecedente("antecedentesmedicos2", "ESTOY EN CONTROL MEDICO :", "col-12", "cmdm", antecedente.ControlMedicoDiabetesMelitus, "S", "rcmdm", antecedente.OpcionesControlMedicoDiabetes, "N", null)
			agregarhr("antecedentesmedicos2")
			agregarAntecedente("antecedentesmedicos2", "ENFERMEDADES DEL COLESTEROL Y TRIGLICERIDOS :", "col-12", "efct", antecedente.EnfermedadColesterol, "N", "refct", null, "N", null)
			agregarAntecedenteT("antecedentesmedicos2", "MEDICAMENTOS : " + antecedente.MedicamentosColesterol, "col-12")
			agregarhr("antecedentesmedicos2")
			agregarAntecedente("antecedentesmedicos2", "ENFERMEDADES DEL ACIDO URICO :", "col-12", "efau", antecedente.EnfermedadAcidoUrico, "N", "refau", null, "N", null)
			agregarhr("antecedentesmedicos2")
			agregarAntecedenteT("antecedentesmedicos2", "MEDICAMENTOS : " + antecedente.MedicamentosAcidoUrico, "col-12")
			agregarhr("antecedentesmedicos2")
			agregarAntecedente("antecedentesmedicos2", "ENFERMEDADES DEL RIÑON o UROLOGICAS :", "col-12", "efr", antecedente.EnfermedadRinon, "S", "refr", antecedente.OpcionesRinones, "N", null)
			agregarhr("antecedentesmedicos2")
			agregarAntecedente("antecedentesmedicos2", "ENFERMEDADES NEUROLOGICAS :", "col-12", "efr", antecedente.EnfermedadNeurologica, "S", "refr", antecedente.OpcionesEnfermedadNuerologicas, "N", null)
			agregarhr("antecedentesmedicos2")
			agregarAntecedente("antecedentesmedicos2", "ENFERMEDADES PSIQUIATRICAS :", "col-12", "efps", antecedente.EnfermedadesPsiquiatricas, "S", "refps", antecedente.OpcionesEnfermedadesPsiquiatricas, "N", null)
			agregarhr("antecedentesmedicos2")
			agregarAntecedente("antecedentesmedicos2", "ENFERMEDADES OSTEO/MUSCULARES HUESOS Y/O MUSCULOS :", "col-12", "efr", antecedente.EnfermedadOsteoMuscular, "S", "refr", antecedente.OpcionesEnfermedadesOsteoMuscular, "N", null)
			agregarhr("antecedentesmedicos2")
			const enfermtext = []; const enfermresp = [];
			enfermtext[0] = "ENFERMEDAD DE LA PIEL :"; enfermresp[0] = antecedente.EnfermedadPiel;
			enfermtext[1] = "ENFERMEDAD DE LA UÑAS :"; enfermresp[1] = antecedente.EnfermedadUñas;
			enfermtext[2] = "ENFERMEDAD En el PELO :"; enfermresp[2] = antecedente.EnfermedadCabello;
			agregarAntecedente_3col("antecedentesmedicos2", "col-3", enfermtext, enfermresp, null)
			agregarhr("antecedentesmedicos2")
			agregarAntecedente("antecedentesmedicos2", "ENFERMEDADES INFECCIOSAS :", "col-12", "efi", antecedente.EnfermedadesInfecciosas, "S", "refr", antecedente.OpcionesEnfermedadesInfecciosas, "N", null)
			agregarhr("antecedentesmedicos2")
			const accidmtext = []; const accidresp = [];
			accidmtext[0] = "ACCIDENTES DE IMPORTANCIA :"; accidresp[0] = antecedente.AccidentesdeImportancia;
			accidmtext[1] = "A RECIBIDO UNA TRANSFUSION DE SANGRE :"; accidresp[1] = antecedente.TranfusionDeSangre;

			agregarAntecedente_3col("antecedentesmedicos2", "col-6", accidmtext, accidresp, null)

			agregarAntecedente("antecedentesmedicos2", "TIENE USTED ALGUNA ENFERMEDAD CRONICA :", "col-12", "efcr", antecedente.EnfermedadCronica, "S", "refcr", antecedente.OpcionesEnfermedadCronica, "S", antecedente.OtraEnfermedadCronica)
			agregarhr("antecedentesmedicos2")
			agregarAntecedente("antecedentesmedicos2", "HA PADECIDO CANCER Y/O TUMOR MALIGNO :", "col-12", "tas", antecedente.PadecidoCancerTumorMaligno, "N", "rtas", null, "N", null)
			agregarhr("antecedentesmedicos2")
			agregarAntecedente2("antecedentesmedicos2", "INGRESO AL HOSPITAL :", "col-4", "ih", antecedente.IngresoHospital, "S", "col-8", "rih", antecedente.OpcionesIngresoHospital)
			agregarAntecedenteT2("antecedentesmedicos2", "CIRUGIAS REALIZADAS : " + antecedente.CirugiasRealizadas, "col-4", "s", "USTED TIENE ALGUNA SECUELA :", antecedente.TieneSecuela, "col-8")
			agregarAntecedente("antecedentesmedicos2", "TIENE USTED ALGUN IMPEDIMENTO FISICO Y/O PSICOLOGICO-EMOCIANAL :", "col-12", "tas", antecedente.ImpedimentoFisicoPsicologico, "N", "rtas", null, "N", null)
			agregarhr("antecedentesmedicos2")
			agregarAntecedente("antecedentesmedicos2", "TIENE USTED VARICES :", "col-12", "tas", antecedente.TieneVarices, "N", "rtas", null, "N", null)


			//habitos
			//tabaco
			switch (antecedente.Tabaco) {

				case "1": {
					document.getElementById('nuncaFumo').setAttribute('style', 'text-decoration:line-through solid 1px cornflowerblue;font-weight: bold;');
					break;
				}
				case "2": {
					document.getElementById('dejeFumar').setAttribute('style', 'text-decoration:line-through solid 1px cornflowerblue;font-weight: bold;');
					document.getElementById('fechaDejeFumar').innerHTML = antecedente.FechaDejeFumar;
					break;
				}
				case "3": {
					document.getElementById('habitualmenteFumo').setAttribute('style', 'text-decoration:line-through solid 1px cornflowerblue;font-weight: bold;');
					break;
				}
				case "4": {
					document.getElementById('diariamente').setAttribute('style', 'text-decoration:line-through solid 1px cornflowerblue;font-weight: bold;');
					document.getElementById('diaramenteFumo').innerHTML = antecedente.DiariamenteFumo;
					break;
				}
				case "5": {
					document.getElementById('semanalmente').setAttribute('style', 'text-decoration:line-through solid 1px cornflowerblue;font-weight: bold;');
					document.getElementById('semanalmenteFumo').innerHTML = antecedente.SemanalmenteFumo;
					break;
				}
			}
			//alcohol
			switch (antecedente.Alcohol) {

				case "1": {
					document.getElementById('nunca').setAttribute('style', 'text-decoration:line-through solid 1px cornflowerblue;font-weight: bold;');
					break;
				}
				case "2": {
					document.getElementById('esporadicamente').setAttribute('style', 'text-decoration:line-through solid 1px cornflowerblue;font-weight: bold;');
					break;
				}
				case "3": {
					document.getElementById('finesSemana').setAttribute('style', 'text-decoration:line-through solid 1px cornflowerblue;font-weight: bold;');
					break;
				}
				case "4": {
					document.getElementById('cualquierDia').setAttribute('style', 'text-decoration:line-through solid 1px cornflowerblue;font-weight: bold;');
					break;
				}
				case "5": {
					document.getElementById('finAño').setAttribute('style', 'text-decoration:line-through solid 1px cornflowerblue;font-weight: bold;');
					break;
				}
			}

			switch (antecedente.TipoBebida) {

				case "1": {
					document.getElementById('cerveza').setAttribute('style', 'text-decoration:line-through solid 1px cornflowerblue;font-weight: bold;');
					break;
				}
				case "2": {
					document.getElementById('vinos').setAttribute('style', 'text-decoration:line-through solid 1px cornflowerblue;font-weight: bold;');
					break;
				}
				case "3": {
					document.getElementById('cualquier').setAttribute('style', 'text-decoration:line-through solid 1px cornflowerblue;font-weight: bold;');
					break;
				}
			}

			//drogas
			switch (antecedente.Drogas) {

				case "1": {
					document.getElementById('dnunca').setAttribute('style', 'text-decoration:line-through solid 1px cornflowerblue;font-weight: bold;');
					break;
				}
				case "2": {
					document.getElementById('desporadicamente').setAttribute('style', 'text-decoration:line-through solid 1px cornflowerblue;font-weight: bold;');
					document.getElementById('tipoDrogas').innerHTML = antecedente.TipoDrogras;
					break;
				}
				case "3": {
					document.getElementById('dcualquier').setAttribute('style', 'text-decoration:line-through solid 1px cornflowerblue;font-weight: bold;');
					document.getElementById('tipoDrogas').innerHTML = antecedente.TipoDrogras;
					break;
				}
				case "4": {
					document.getElementById('ddejeconsumir').setAttribute('style', 'text-decoration:line-through solid 1px cornflowerblue;font-weight: bold;');
					document.getElementById('fechaDejeConsumir').innerHTML = antecedente.FechaDejeDrogas;
					document.getElementById('tipoDrogas').innerHTML = antecedente.TipoDrogras;
					break;
				}
				case "5": {
					document.getElementById('dsoloprobe').setAttribute('style', 'text-decoration:line-through solid 1px cornflowerblue;font-weight: bold;');
					document.getElementById('tipoDrogas').innerHTML = antecedente.TipoDrogras;
					break;
				}
			}

			//deportes
			switch (antecedente.DeportesActividadFisica) {

				case "1": {
