					document.getElementById('tipoEjercicio').innerHTML = antecedente.TipoEjercicio;
					document.getElementById('TiempoDedicado').innerHTML = antecedente.TipoDrogras;
					break;
				}
			}

			//USO DE MEDICAMENTOS
			if (antecedente.UsoMedicamentos === "S") {
				asignarCheck("MS")
				document.getElementById('nombreMedicamento').innerHTML = antecedente.NombreMedicamento;
