			ginecologico = cuestionario.CuestionarioMedico.AntecedentesGinecologicos;
			//solo si es mujer
			document.getElementById('embarazos').innerHTML = ginecologico.Embarazos;
			document.getElementById('partos').innerHTML = ginecologico.Nacimientos;
			document.getElementById('cesarias').innerHTML = ginecologico.Cesarias;
			document.getElementById('abortos').innerHTML = ginecologico.Abortos;
