	<script type="text/javascript">

		window.chrome.webview.addEventListener('message', event => {
			WriteDataFromCsharp(event.data);
		});
		function WriteDataFromCsharp(data) {
			candidato = JSON.parse(data);
			//firmas reprecentante legal,patron,testigo,empleado
			document.getElementById('firmaPatron').setAttribute("src", candidato.FirmasRepTes[0].Firma);
			document.getElementById('nombrePatron').innerHTML = candidato.FirmasRepTes[0].Nombre;
			document.getElementById('firmaReprecentante').setAttribute("src", candidato.FirmasRepTes[0].Firma);
			document.getElementById('nombreReprecentante').innerHTML = candidato.FirmasRepTes[0].Nombre;
			document.getElementById('firmaTestigo').setAttribute("src", candidato.FirmasRepTes[1].Firma);
			document.getElementById('nombreTestigo').innerHTML = candidato.FirmasRepTes[1].Nombre;
			document.getElementById('firma_ContratoLaboral').setAttribute("src", candidato.Firma);
			document.getElementById('nombre_Contrato').innerText = candidato.Nombre;

			document.getElementById('nombre_contrato').innerText = candidato.Nombre;
			document.getElementById('nombre_contrato2').innerText = candidato.Nombre;

			document.getElementById('sexo').innerText = getSexo(candidato.Persona.Sexo);
			document.getElementById('edad').innerText = candidato.Persona.Edad;
			document.getElementById('estadoCivil').innerText = candidato.Persona.EstadoCivil;
			document.getElementById('curp').innerText = candidato.Persona.Curp;
			document.getElementById('rfc').innerText = candidato.Persona.Rfc;
			document.getElementById('domicilio').innerText = candidato.Persona.Domicilio;
			document.getElementById('puesto').innerText = candidato.Persona.Puesto;
			document.getElementById('sueldo').innerText = candidato.Persona.Sueldo;
			add_Beneficiarios(candidato.Beneficiario)
			formatoDocumento(candidato.Fecha);
			//huellas dactilares de los pulgares
			document.getElementById('fingerRight').setAttribute("src", candidato.Huellas.HuellaD)

		}
		function getSexo(_sexo) {
			if (_sexo === "M") {
				return "Masculino";
			} else {
				return "Femenino";
			}
		}
		function formatoDocumento(fecha_documento) {
			var fechayhora = new Array()
			fechayhora = fecha_documento.split(' ');
			var fecha = new Array()
			fecha = fechayhora[0].split('/')
			var dia = fecha[0]
			var mes = fecha[1]
			var año = fecha[2]

			document.getElementById('dia').innerText = dia
			document.getElementById('mes').innerText = gmes(mes)
			document.getElementById('año').innerText = año

		}
		function gmes(mes) {
			var _mes = ""
			switch (mes) {
				case "01":
					_mes = "Enero"
					break;
				case "02":
					_mes = "Febrero"
					break;
				case "03":
					_mes = "Marzo"
					break;
				case "04":
					_mes = "Abril"
					break;
				case "05":
					_mes = "Mayo"
					break;
				case "06":
					_mes = "Junio"
					break;
				case "07":
					_mes = "Julio"
					break;
				case "08":
					_mes = "Agosto"
					break;
				case "09":
					_mes = "Septiembre"
					break;
				case "10":
					_mes = "Octubre"
					break;
				case "11":
					_mes = "Noviembre"
					break;
				case "12":
					_mes = "Diciembre"
					break;
				default:
					break;
			}

			return _mes;
		}
		function RequestData() {
			//function triggered by a button on the html page
			//alert('post to c#');
			window.chrome.webview.postMessage('getData');
		}

		function add_Beneficiarios(beneficiarios) {
			//var storedArray = JSON.parse(sessionStorage.getItem("arrBeneficiarios"));
			var storedArray = JSON.parse(beneficiarios);
			var i;
			for (i = 0; i < storedArray.length; i++) {
				console.log(storedArray[i][0]);
				var tr = document.createElement('tr');
				var thnombre = document.createElement("th");
				var spnombre = document.createElement("p");
				spnombre.innerText = storedArray[i][0];
				thnombre.append(spnombre);
				tr.append(thnombre);
				var thparentesco = document.createElement("th");
				var spparentesco = document.createElement("p");
				spparentesco.innerText = storedArray[i][1];
				thparentesco.append(spparentesco);
				tr.append(thparentesco);
				var thporcentaje = document.createElement("th");
				var spporcentaje = document.createElement("p");
				spporcentaje.innerText = storedArray[i][2] + "%";
				thporcentaje.append(spporcentaje);
				tr.append(thporcentaje);

				document.getElementById('beneficiarios').append(tr)
			}
		}

	</script>
